<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE IdentityTrigger PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<IdentityTrigger disabled="true" handler="sailpoint.api.WorkflowTriggerHandler" name="Joiner-JDBC" type="Create">
  <Description>Create JDBC account to identity</Description>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <HandlerParameters>
    <Attributes>
      <Map>
        <entry key="workflow" value="MyJoiner-JDBC"/>
      </Map>
    </Attributes>
  </HandlerParameters>
</IdentityTrigger>