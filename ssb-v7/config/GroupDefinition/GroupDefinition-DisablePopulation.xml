    <CompositeFilter operation="AND">
      <Filter operation="COLLECTION_CONDITION" property="links">
        <CollectionCondition>
          <CompositeFilter operation="AND">
            <CompositeFilter operation="AND">
              <Filter operation="EQ" property="application.name" value="AD"/>
            </CompositeFilter>
          </CompositeFilter>
        </CollectionCondition>
      </Filter>
    </CompositeFilter>
  </GroupFilter>
